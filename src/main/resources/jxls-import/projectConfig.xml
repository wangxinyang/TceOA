<?xml version="1.0" encoding="UTF-8"?>
<workbook>
    <worksheet name="Sheet1">
        <section startRow="0" endRow="0"/>
        <loop startRow="1" endRow="1" items="projectInfoList" var="projectInfo" varType="com.tce.oa.modular.company.model.Project">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">projectInfo.code</mapping>
                <mapping row="1" col="1">projectInfo.name</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>